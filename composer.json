{
    "name": "technopers/laravel-trackable-jobs",
    "description": "",
    "keywords": [
        "technopers",
        "laravel-trackable-jobs"
    ],
    "homepage": "https://github.com/technopers/laravel-trackable-jobs",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Hardik Modha",
            "email": "hardik@technopers.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.28",
        "spatie/laravel-package-tools": "^1.13"
    },
    "require-dev": {
        "orchestra/testbench": "^7.7",
        "phpunit/phpunit": "^9.5.24"
    },
    "autoload": {
        "psr-4": {
            "Technopers\\LaravelTrackableJobs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Technopers\\LaravelTrackableJobs\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Technopers\\LaravelTrackableJobs\\LaravelTrackableJobsServiceProvider"
            ]
        }
    }
}
