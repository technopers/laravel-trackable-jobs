{
    "name": "technopers/laravel-trackable-jobs",
    "description": "",
    "keywords": [
        "technopers",
        "laravel-trackable-jobs"
    ],
    "homepage": "https://github.com/technopers/laravel-trackable-jobs",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Hardik Modha",
            "email": "hardik@technopers.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "spatie/laravel-package-tools": "^1.14"
    },
    "require-dev": {
        "orchestra/testbench": "^v8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Technopers\\LaravelTrackableJobs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Technopers\\LaravelTrackableJobs\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Technopers\\LaravelTrackableJobs\\LaravelTrackableJobsServiceProvider"
            ]
        }
    }
}
